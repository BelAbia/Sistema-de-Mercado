<mvc:View
	controllerName="sap.ui.demo.walkthrough.controller.Detalhes"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<Page
		title="{i18n>TituloTelaDeDetalhes}"
		showNavButton="true"
		navButtonPress="AoClicarNoBotaoDeVoltar">
		<ObjectHeader
			id="objectHeader"
			intro="{detalhes>nome}"
			title="{detalhes>marca}">
		<attributes>
        	<ObjectAttribute title="{i18n>DetalheCodigoDeBarras}" text = "{detalhes>codigoBarras}" />
			<ObjectAttribute title="data" text="{detalhes>dataCadastro}"/>
   		</attributes>
</ObjectHeader>
	</Page>
</mvc:View>